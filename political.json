{
  "version": 8,
  "name": "OHM Political",
  "metadata": {
    "maputnik:license": "https://github.com/maputnik/osm-liberty/blob/gh-pages/LICENSE.md",
    "maputnik:renderer": "mbgljs",
    "openmaptiles:version": "3.x"
  },
  "sources": {
    "openmaptiles": {
      "type": "vector",
      "url": "https://api.maptiler.com/tiles/v3/tiles.json?key=q9mnFdtg2QxTwjYaTGjp"
    },
    "natural_earth_shaded_relief": {
      "maxzoom": 6,
      "tileSize": 256,
      "tiles": [
        "https://klokantech.github.io/naturalearthtiles/tiles/natural_earth_2_shaded_relief.raster/{z}/{x}/{y}.png"
      ],
      "type": "raster"
    },
    "ohm": {
      "type": "vector",
      "tiles": [
        "https://tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=boundary_label,culture_label,waterway,building,industrial,place,building_label,landuse",
        "https://a.tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=boundary_label,culture_label,waterway,building,industrial,place,building_label,landuse",
        "https://b.tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=boundary_label,culture_label,waterway,building,industrial,place,building_label,landuse",
        "https://c.tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=boundary_label,culture_label,waterway,building,industrial,place,building_label,landuse"
      ],
      "minZoom": 0,
      "maxZoom": 14,
      "maxzoom": 25,
      "minzoom": 3
    },
    "ohm-ephemeral": {
      "type": "vector",
      "tiles": [
        "https://tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=movement,event",
        "https://a.tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=movement,event",
        "https://b.tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=movement,event",
        "https://a.tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=movement,event"
      ],
      "minZoom": 0,
      "maxZoom": 14,
      "maxzoom": 25,
      "minzoom": 3
    },
    "ohm-boundaries": {
      "type": "vector",
      "tiles": [
        "https://tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=boundary,culture,water,landuse",
        "https://a.tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=boundary,culture,water,landuse",
        "https://b.tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=boundary,culture,water,landuse",
        "https://c.tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=boundary,culture,water,landuse"
      ],
      "minZoom": 0,
      "maxZoom": 14,
      "maxzoom": 25
    },
    "ohm-transportation": {
      "type": "vector",
      "tiles": [
        "https://tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=transportation",
        "https://a.tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=transportation",
        "https://b.tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=transportation",
        "https://c.tiles.openhistorymap.org/items/{atDate}/{z}/{y}/{x}/vector.pbf?layers=transportation"
      ],
      "minZoom": 0,
      "maxZoom": 14,
      "maxzoom": 25,
      "minzoom": 3
    }
  },
  "sprite": "https://maputnik.github.io/osm-liberty/sprites/osm-liberty",
  "glyphs": "https://api.maptiler.com/fonts/{fontstack}/{range}.pbf?key=q9mnFdtg2QxTwjYaTGjp",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "minzoom": 0,
      "filter": ["any"],
      "paint": {"background-color": "rgba(208, 236, 179, 1)"}
    },
    {
      "id": "natural_earth",
      "type": "raster",
      "source": "natural_earth_shaded_relief",
      "maxzoom": 6,
      "paint": {"raster-opacity": {"base": 1.5, "stops": [[0, 0.8], [6, 0.1]]}}
    },
    {
      "id": "landcover_wood",
      "type": "fill",
      "source": "ohm",
      "source-layer": "landcover",
      "filter": ["all", ["==", "class", "wood"]],
      "paint": {
        "fill-antialias": false,
        "fill-color": "hsla(98, 61%, 72%, 0.7)",
        "fill-opacity": 0.4
      }
    },
    {
      "id": "landcover_ice",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["all", ["==", "class", "ice"]],
      "paint": {
        "fill-antialias": false,
        "fill-color": "rgba(224, 236, 236, 1)",
        "fill-opacity": 0.8
      }
    },
    {
      "id": "green_garden-copy",
      "type": "fill",
      "source": "ohm",
      "source-layer": "landuse",
      "filter": ["all", ["==", "building", "park"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-antialias": true,
        "fill-color": "rgba(29, 94, 32, 1)",
        "fill-opacity": 0.3
      }
    },
    {
      "id": "green_grass",
      "type": "fill",
      "source": "ohm",
      "source-layer": "landuse",
      "filter": ["all", ["==", "building", "grass"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-antialias": false,
        "fill-color": "rgba(108, 185, 70, 1)",
        "fill-opacity": 0.3
      }
    },
    {
      "id": "green_garden",
      "type": "fill",
      "source": "ohm",
      "source-layer": "landuse",
      "filter": ["all", ["==", "building", "garden"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-antialias": true,
        "fill-color": "rgba(100, 131, 46, 1)",
        "fill-opacity": 0.3,
        "fill-outline-color": "rgba(174, 95, 95, 1)"
      }
    },
    {
      "id": "landuse_residential",
      "type": "fill",
      "source": "ohm",
      "source-layer": "landuse",
      "maxzoom": 11,
      "filter": ["==", "class", "residential"],
      "paint": {
        "fill-color": {
          "base": 1,
          "stops": [
            [9, "rgba(199, 199, 199, 0.84)"],
            [12, "hsla(35, 57%, 88%, 0.49)"]
          ]
        }
      }
    },
    {
      "id": "landuse_cemetery",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "filter": ["==", "class", "cemetery"],
      "paint": {"fill-color": "hsl(75, 37%, 81%)"}
    },
    {
      "id": "countries",
      "type": "fill",
      "source": "ohm-boundaries",
      "source-layer": "boundary",
      "minzoom": 0,
      "maxzoom": 12,
      "filter": ["all", ["==", "admin_level", 2]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": {"type": "identity", "property": "color"},
        "fill-opacity": {"stops": [[0, 0.8], [6, 0.7], [7, 0.4], [10, 0.2]]},
        "fill-antialias": true,
        "fill-translate": [0, 0]
      }
    },
    {
      "id": "waterway_tunnel",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "filter": ["all", ["==", "brunnel", "tunnel"]],
      "paint": {
        "line-color": "#a0c8f0",
        "line-dasharray": [3, 3],
        "line-gap-width": {"stops": [[12, 0], [20, 6]]},
        "line-opacity": 1,
        "line-width": {"base": 1.4, "stops": [[8, 1], [20, 2]]}
      }
    },
    {
      "id": "waterway_river",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "filter": ["all", ["==", "class", "river"], ["!=", "brunnel", "tunnel"]],
      "layout": {"line-cap": "round"},
      "paint": {
        "line-color": "#a0c8f0",
        "line-width": {"base": 1.2, "stops": [[11, 0.5], [20, 6]]}
      }
    },
    {
      "id": "waterway_other",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "filter": ["all", ["!=", "class", "river"], ["!=", "brunnel", "tunnel"]],
      "layout": {"line-cap": "round"},
      "paint": {
        "line-color": "#a0c8f0",
        "line-width": {"base": 1.3, "stops": [[13, 0.5], [20, 6]]}
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "filter": ["all", ["!=", "brunnel", "tunnel"]],
      "paint": {"fill-color": "rgb(158,189,255)"}
    },
    {
      "id": "landcover_sand",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["all", ["==", "class", "sand"]],
      "paint": {"fill-color": "rgba(247, 239, 195, 1)"}
    },
    {
      "id": "aeroway_fill",
      "type": "fill",
      "source": "ohm",
      "source-layer": "aeroway",
      "minzoom": 11,
      "filter": ["==", "$type", "Polygon"],
      "paint": {"fill-color": "rgba(229, 228, 224, 1)", "fill-opacity": 0.7}
    },
    {
      "id": "aeroway_runway",
      "type": "line",
      "source": "ohm",
      "source-layer": "aeroway",
      "minzoom": 11,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["==", "class", "runway"]
      ],
      "paint": {
        "line-color": "#f0ede9",
        "line-width": {"base": 1.2, "stops": [[11, 3], [20, 16]]}
      }
    },
    {
      "id": "aeroway_taxiway",
      "type": "line",
      "source": "ohm",
      "source-layer": "aeroway",
      "minzoom": 11,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["==", "class", "taxiway"]
      ],
      "paint": {
        "line-color": "#f0ede9",
        "line-width": {"base": 1.2, "stops": [[11, 0.5], [20, 6]]}
      }
    },
    {
      "id": "tunnel_minor",
      "type": "line",
      "source": "ohm",
      "source-layer": "transportation",
      "filter": ["all", ["==", "brunnel", "tunnel"], ["in", "class", "minor"]],
      "layout": {"line-join": "round"},
      "paint": {
        "line-color": "#fff",
        "line-width": {"base": 1.2, "stops": [[13.5, 0], [14, 2.5], [20, 11.5]]}
      }
    },
    {
      "id": "tunnel_major_rail",
      "type": "line",
      "source": "ohm",
      "source-layer": "transportation",
      "filter": ["all", ["==", "brunnel", "tunnel"], ["in", "class", "rail"]],
      "paint": {
        "line-color": "#bbb",
        "line-width": {"base": 1.4, "stops": [[14, 0.4], [15, 0.75], [20, 2]]}
      }
    },
    {
      "id": "tunnel_transit_rail",
      "type": "line",
      "source": "ohm",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["==", "brunnel", "tunnel"],
        ["in", "class", "transit"]
      ],
      "paint": {
        "line-color": "#bbb",
        "line-width": {"base": 1.4, "stops": [[14, 0.4], [15, 0.75], [20, 2]]}
      }
    },
    {
      "id": "road_area",
      "type": "fill",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": ["all", [">", "$area", 0]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(207, 188, 115, 1)"}
    },
    {
      "id": "road_motorway_link_casing",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "minzoom": 12,
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["==", "class", "motorway"],
        ["==", "ramp", 1],
        ["==", "$area", 0]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#e9ac77",
        "line-width": {
          "base": 1.2,
          "stops": [[12, 1], [13, 3], [14, 4], [20, 15]]
        }
      }
    },
    {
      "id": "road_service_track_casing",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["in", "class", "service", "track"],
        ["==", "$area", 0]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#cfcdca",
        "line-width": {"base": 1.2, "stops": [[15, 1], [16, 4], [20, 11]]}
      }
    },
    {
      "id": "road_link_casing",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["!in", "class", "pedestrian", "path", "track", "service", "motorway"],
        ["==", "ramp", 1]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#e9ac77",
        "line-width": {
          "base": 1.2,
          "stops": [[12, 1], [13, 3], [14, 4], [20, 15]]
        }
      }
    },
    {
      "id": "road_secondary_tertiary_casing",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["in", "class", "secondary", "tertiary"],
        ["!=", "ramp", 1],
        ["==", "$area", 0]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#e9ac77",
        "line-width": {"base": 1.2, "stops": [[8, 1.5], [20, 17]]}
      }
    },
    {
      "id": "road_trunk_primary_casing",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["in", "class", "primary", "trunk"]
      ],
      "layout": {"line-join": "round"},
      "paint": {
        "line-color": "#e9ac77",
        "line-width": {
          "base": 1.2,
          "stops": [[5, 0.4], [6, 0.7], [7, 1.75], [20, 22]]
        }
      }
    },
    {
      "id": "road_motorway_casing",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "minzoom": 5,
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["==", "class", "primary"],
        ["!=", "ramp", 1]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#e9ac77",
        "line-width": {
          "base": 1.2,
          "stops": [[5, 0.4], [6, 0.7], [7, 1.75], [20, 22]]
        }
      }
    },
    {
      "id": "road_path_pedestrian",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "minzoom": 14,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["!in", "brunnel", "bridge", "tunnel"],
        ["in", "class", "path", "pedestrian"]
      ],
      "layout": {"line-join": "round"},
      "paint": {
        "line-color": "hsl(0, 0%, 100%)",
        "line-dasharray": [1, 0.7],
        "line-width": {"base": 1.2, "stops": [[14, 1], [20, 10]]}
      }
    },
    {
      "id": "road_motorway_link",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "minzoom": 12,
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["==", "class", "motorway"],
        ["==", "ramp", 1]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#fc8",
        "line-width": {
          "base": 1.2,
          "stops": [[12.5, 0], [13, 1.5], [14, 2.5], [20, 11.5]]
        }
      }
    },
    {
      "id": "road_service_track",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["in", "class", "service", "track"]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "#fff",
        "line-width": {"base": 1.2, "stops": [[15.5, 0], [16, 2], [20, 7.5]]}
      }
    },
    {
      "id": "road_secondary_tertiary",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["in", "class", "secondary", "tertiary"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "none"
      },
      "paint": {
        "line-color": "#fea",
        "line-width": {"base": 1.2, "stops": [[6.5, 0], [8, 0.5], [20, 13]]}
      }
    },
    {
      "id": "road_trunk_primary",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["in", "class", "primary", "trunk"]
      ],
      "layout": {"line-join": "round"},
      "paint": {
        "line-color": "#fea",
        "line-width": {"base": 1.2, "stops": [[5, 0], [7, 1], [20, 18]]}
      }
    },
    {
      "id": "road_major_rail",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "minzoom": 3,
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["==", "class", "rail"]
      ],
      "paint": {
        "line-color": "#bbb",
        "line-width": {"base": 1.4, "stops": [[14, 0.4], [15, 0.75], [20, 2]]}
      }
    },
    {
      "id": "road_major_rail_hatching",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["==", "class", "rail"]
      ],
      "paint": {
        "line-color": "#bbb",
        "line-dasharray": [0.2, 8],
        "line-width": {"base": 1.4, "stops": [[14.5, 0], [15, 3], [20, 8]]}
      }
    },
    {
      "id": "road_transit_rail",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["==", "class", "transit"]
      ],
      "paint": {
        "line-color": "#bbb",
        "line-width": {"base": 1.4, "stops": [[14, 0.4], [15, 0.75], [20, 2]]}
      }
    },
    {
      "id": "road_transit_rail_hatching",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["!in", "brunnel", "bridge", "tunnel"],
        ["==", "class", "transit"]
      ],
      "paint": {
        "line-color": "#bbb",
        "line-dasharray": [0.2, 8],
        "line-width": {"base": 1.4, "stops": [[14.5, 0], [15, 3], [20, 8]]}
      }
    },
    {
      "id": "road_minor",
      "type": "line",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": ["any", ["==", "class", "tertiary"], ["==", "$area", 0]],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "none"
      },
      "paint": {
        "line-color": "rgba(197, 185, 131, 1)",
        "line-width": {"base": 1.2, "stops": [[13.5, 0], [14, 2.5], [20, 18]]}
      }
    },
    {
      "id": "road_area_label",
      "type": "symbol",
      "source": "ohm-transportation",
      "source-layer": "transportation",
      "minzoom": 7,
      "filter": ["any", ["==", "class", "tertiary"]],
      "layout": {
        "visibility": "visible",
        "text-field": "{name}",
        "text-font": ["Open Sans Regular", "Arial Unicode MS Regular"],
        "symbol-placement": "line",
        "text-justify": "auto",
        "symbol-z-order": "auto",
        "text-size": 10
      },
      "paint": {"text-translate": [5, 1], "text-translate-anchor": "viewport"}
    },
    {
      "id": "building_roof",
      "type": "fill",
      "source": "ohm",
      "source-layer": "building",
      "minzoom": 11,
      "maxzoom": 24,
      "filter": ["all", ["==", "building", "roof"]],
      "paint": {
        "fill-color": "rgba(182, 128, 51, 1)",
        "fill-outline-color": {
          "base": 1,
          "stops": [[13, "hsla(35, 6%, 79%, 0.32)"], [14, "hsl(35, 6%, 79%)"]]
        },
        "fill-opacity": 0.2
      }
